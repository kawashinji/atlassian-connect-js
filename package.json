{
  "name": "atlassian-connect-js",
  "version": "5.3.40",
  "license": "Apache-2.0",
  "description": "Atlassian Connect JavaScript bridge",
  "main": "dist/connect-host.js",
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.org:atlassian/atlassian-connect-js.git"
  },
  "scripts": {
    "bundlesize": "bundlesize --config=bundlesize.config.json",
    "bundlesize:all": "npm run bundlesize; npm run bundlesize:nozip",
    "bundlesize:nozip": "bundlesize --config=bundlesize-nozip.config.json",
    "depcheck": "depcheck --config=.depcheckrc.json",
    "test-sl": "SAUCE_LABS=true SAUCE_USERNAME=$SAUCE_USERNAME SAUCE_ACCESS_KEY=$SAUCE_ACCESS_KEY npm test",
    "prepublish": "node_modules/gulp/bin/gulp.js",
    "test": "gulp test-ci",
    "test-mem": "cd memleak-check && node index",
    "tag": "PACKAGE_VERSION=$(node -p \"require('./package.json').version\") && git tag $PACKAGE_VERSION && git push --tags"
  },
  "devDependencies": {
    "@atlassian/eslint-config-atlassian-fecq": "2.0.3",
